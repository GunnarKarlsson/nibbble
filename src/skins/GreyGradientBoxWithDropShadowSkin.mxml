<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" xmlns:styles="styles.*">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.SkinnableContainer")]
	</fx:Metadata>
	
	<fx:Declarations>
		<s:DropShadowFilter id="dropShadowFilter"/>
		<styles:GraphicElementsLibrary id="graphicsLib"/>
	</fx:Declarations>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="normal" />
	</s:states>
	
		<s:Rect width="100%" height="100%" filters="{[graphicsLib.getDropShadowFilter()]}">
			<s:fill>
				<s:LinearGradient>
					<s:GradientEntry color="#bbbbbb"/>
					<s:GradientEntry color="#aaaaaa"/>
				</s:LinearGradient>
			</s:fill>
		</s:Rect>
	
	<s:Group id="contentGroup"/>
	
	<!-- SkinParts
	name=contentGroup, type=spark.components.Group, required=false
	-->
</s:Skin>
