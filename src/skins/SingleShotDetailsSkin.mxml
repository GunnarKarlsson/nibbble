<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.SkinnableContainer")]
	</fx:Metadata>
	
	<fx:Declarations>
		<s:DropShadowFilter id="dropShadow"/>
	</fx:Declarations>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="normal" />
	</s:states>
	
	<s:Path 
		filters="{[dropShadow]}"
		data="M 0 20
			L 180 20
			L 200 0
			L 220 20
			L 400 20
			L 400 200
			L 0 200
			L 0 0" >
		<s:fill>
			<s:LinearGradient>
				<s:GradientEntry color="#eeeeee"/>
				<s:GradientEntry color="#dddddd"/>
			</s:LinearGradient>
		</s:fill>
	</s:Path>
	
	<s:Rect id="rect" width="100%" height="100%" visible="false"/>
	
	<s:Group id="contentGroup">
	</s:Group>   
	
	<!-- SkinParts
	name=contentGroup, type=spark.components.Group, required=false
	-->
</s:Skin>
