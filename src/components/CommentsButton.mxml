<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		width="154" height="45">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:Rect x="0.5" y="0.5" width="153" height="44" radiusX="4">
		<s:stroke>
			<s:LinearGradientStroke caps="none" joints="miter" miterLimit="4" rotation="90"
									weight="1">
				<s:GradientEntry alpha="1.0" color="#999999" ratio="0.027450980392156862"/>
				<s:GradientEntry alpha="1.0" color="#666666" ratio="1"/>
			</s:LinearGradientStroke>
		</s:stroke>
		<s:fill>
			<s:LinearGradient rotation="90">
				<s:GradientEntry alpha="1.0" color="#666666" ratio="0"
								 color.down="#CCCCCC"/>
				<s:GradientEntry alpha="1.0" color="#333333" ratio="1"
								 color.down="#999999"/>
			</s:LinearGradient>
		</s:fill>
	</s:Rect>
	<s:RichText x="71" y="15" color="#FFFFFF" fontFamily="Arial" fontSize="16" tabStops="S0"
				text="Button"
				color.down="#333333"/>
	<s:BitmapImage x="5" y="0" width="44" height="44" blendMode="exclusion" smooth="true"
				   source="@Embed('/assets/images/speechBubble.png')"/>
</s:Skin>
